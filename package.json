{
  "name": "map_api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "prod": "npm run build && node dist/entry.js",
    "test": "jest --no-cache",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node src/entry.ts\" -e ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.0",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "supertest": "^4.0.2",
    "ts-node": "^7.0.1"
  }
}
